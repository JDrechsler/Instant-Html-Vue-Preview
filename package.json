{
  "name": "instant-html-vue-preview",
  "description": "Instant Preview HTML / Vue Files",
	"displayName": "Instant Preview HTML / Vue Files",
  "version": "0.0.4",
  "publisher": "JDrechslerUS",
  "engines": {
    "vscode": "^1.26.0"
  },
  "repository": {
    "url": "https://github.com/JDrechsler/Vue-Component-Preview"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onWebviewPanel:instantHtmlVueComponent"
  ],
  "main": "./outDir/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "instantHtmlVueComponentPreview.start",
        "title": "Start Preview",
        "category": "Instant Html / Vue Preview"
      }
    ]
  },
  "scripts": {
    "prepack": "vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "@types/node": "20.11.5",
    "@vscode/vsce": "^2.22.0",
    "tslint": "6.1.3",
    "typescript": "5.3.3",
    "vscode": "1.1.37"
  }
}
